lowhar.quant <- t(apply(lowhar, 2, quantile, probs=c(0.025,0.5,0.975)))
tothar <- canhar + midhar + lowhar
tothar.quant <- t(apply(tothar, 2, quantile, probs=c(0.025,0.5,0.975)))

#summarize harvest rate estimates
